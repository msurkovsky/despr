name = Canny ED
description = Algorithm of Canny Edge detector

inputImage = Input image
outputImage = Output image
size = Kernel size
theta = Theta
lowthresh = Low threshold
highthresh = Hight threshold
scale = Scale
offset = Offset
completeSides = Complete sides
extendedBackgroundColor = Background color

exception.output_parameter_set_method = Method should not be ever used!
exception.null_size = Size must not be NULL!
exception.even_size = Size must be only odd integer!
exception.null_theta = Theta must not be NULL!
exception.theta_is_out_of_range = Theta is out of the range <0, Pi>!
exception.null_lowthresh = Low threshold must not be NULL!
exception.lowthresh_out_of_range = Low threshold is out of the range <0, 255>!
exception.null_highthresh = High threshold must not be NULL!
exception.highthresh_out_of_range = High threshold is out of the range <0, 255>!
exception.null_scale = Scale must not be NULL!
exception.negative_scale = Scale must be a positive integer!
exception.negative_scale = Scale must be a positive integer!
exception.null_offset = Offset must not be NULL!
exception.offset_is_out_of_range = Offset is out of the range <0, 255>!
exception.null_complete_sides = Complete sides flag must not be NULL!
exception.null_extended_background_color = Background color must not be NULL!
exception.null_input_image = Input image must not be NULL!